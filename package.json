{
  "name": "kuro-lang",
  "version": "0.0.0",
  "description": "Generic programming language and environment written in TypeScript.",
  "main": "lib/index.js",
  "repository": "https://github.com/quro-framework/kuro-lang",
  "author": "hota1024",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest --coverage",
    "coverage:open": "jest --coverage && open-cli coverage/lcov-report/index.html",
    "test:watch": "jest --watchAll --coverage"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "open-cli": "^6.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.0"
  }
}
